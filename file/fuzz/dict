# Dictionary for file command fuzzing
# Common file magic signatures
# Executables
"\x7fELF"
"MZ"
"\x00\x00\x01\x00"
"\xca\xfe\xba\xbe"
"\xfe\xed\xfa\xce"
"\xce\xfa\xed\xfe"
# Archives
"PK\x03\x04"
"\x1f\x8b"
"BZ"
"Rar!"
"\xfd7zXZ\x00"
"\x1f\x9d"
# Documents
"%PDF"
"\xd0\xcf\x11\xe0"
"<?xml"
"<!DOCTYPE"
"<html"
# Images
"\x89PNG"
"GIF87a"
"GIF89a"
"\xff\xd8\xff"
"RIFF"
"WEBP"
"BM"
# Audio/Video
"ID3"
"ftyp"
"OggS"
"\x00\x00\x00\x1c"
# Text patterns
"#!/bin/sh"
"#!/bin/bash"
"#!/usr/bin/env"
"#!"
"/*"
"//"
"#include"
"import"
"package"
"class"
"function"
"def "
# Magic file keywords
"offset"
"string"
"byte"
"short"
"long"
"quad"
"float"
"double"
"regex"
"default"
"name"
"use"
"indirect"
"clear"
"der"
"guid"
