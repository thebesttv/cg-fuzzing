# ODT file format (OpenDocument Text)
# ODT is a ZIP file containing XML documents

# ZIP signatures
"PK\x03\x04"
"PK\x01\x02"
"PK\x05\x06"

# ODT mimetype (first file in archive)
"application/vnd.oasis.opendocument.text"
"mimetype"

# Main XML files in ODT
"content.xml"
"styles.xml"
"meta.xml"
"settings.xml"
"manifest.xml"
"META-INF/manifest.xml"

# XML declarations
"<?xml"
'version="1.0"'
'encoding="UTF-8"'
"?>"

# OpenDocument namespace prefixes
"office:"
"text:"
"style:"
"table:"
"draw:"
"fo:"
"xlink:"
"dc:"
"meta:"
"svg:"

# Common ODT elements
"<office:document"
"<office:body>"
"<office:text>"
"<text:p"
"<text:h"
"<text:span"
"<text:list"
"<text:list-item"
"<text:a"
"<text:s"
"<text:tab"
"<text:line-break"
"<table:table"
"<table:table-row"
"<table:table-cell"
"<draw:frame"
"<draw:image"

# Closing tags
"</office:document>"
"</office:body>"
"</office:text>"
"</text:p>"
"</text:h>"
"</text:span>"

# Attributes
'text:style-name='
'text:outline-level='
'office:version='
"1.2"
"1.3"

# Special characters
"\n"
"\t"
"&amp;"
"&lt;"
"&gt;"
"&quot;"
"&apos;"

# Edge cases
"\x00"
"\xff"
"]]>"
"<![CDATA["
"-->"
"<!--"
